pipeline {
 agent any 
 stages{
  parallel {
        }
  catchError(buildResult: 'SUCCESS' , stageResult: 'FAILURE') {
      stage ('stage1') {
	steps{
	  sh 'this is stage1'
	  sh 'exit 1'
	  }
	  }
	stage ('stage2') {
	steps { 
	    echo "this is stage2 "
	   sh 'exit1'
	   }
	  }
	  stage ('stage3') {
         steps {
	  sh '''
            #!/bin/bash
             pwd
             ls
         echo "this is stage3"
	 '''
       }
      }
    }
   }
}   
}
