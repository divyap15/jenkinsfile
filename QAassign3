pipeline {
 agent any 
 stages {
      stage ('stage1') {
	parallel {
        catchError(buildResult: 'SUCCESS' , stageResult: 'FAILURE') {
        steps{
	  echo "this is stage1"
	  exi1
	  }
	  }
	stage ('stage2') {
	steps { 
	    echo "this is stage2 "
	   exit1
	   }
	  }
	  }
	 } 
	  stage ('stage3') {
         steps {
	  sh '''
            #!/bin/bash
             pwd
             ls
         echo "this is stage3"
	 '''
       }
      }
    }
   }

