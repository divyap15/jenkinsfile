pipeline {
    agent any {
         environment {
          BRANCH ='master'
          GIT_REPO ='https://github.com/divyap15/jenkinsfile.git'
            stages {
                 stage('TESTING1') {
                 steps {
                        git branch: env.BRANCH, url: env.GIT_REPO
                        if [ git branch -eq master ]
                        then 
                        sh 'echo "TESTING is running on master"'
                        else 
                        catchError(buildResult: 'SUCCESS', stageResult: 'FAILURE')
                        sh 'echo "BRANCH is not a master" '
                        sh 'sleep 5'
                      }
                     }
                  }
             }
         }
       }
